<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="CustomImageView">
    <attr format="color" name="ml_border_color"/>
    <attr format="dimension" name="ml_border_width"/>
    <attr format="integer" name="ml_press_alpha"/>
    <attr format="color" name="ml_press_color"/>
    <attr format="dimension" name="ml_radius"/>
    <attr format="enum" name="ml_shape_type">
      <enum name="none" value="0"/>
      <enum name="round" value="1"/>
      <enum name="rectangle" value="2"/>
    </attr>
  </declare-styleable>

  <!--自定义的领取礼包进度条-->
  <declare-styleable name="RoundedRectProgressBar">
    <attr name="backColor" format="color" />
    <attr name="barColor" format="color" />
    <attr name="textColor" format="color" />
  </declare-styleable>
</resources>